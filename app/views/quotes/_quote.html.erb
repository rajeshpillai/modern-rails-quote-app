<%= turbo_frame_tag quote do %>
  <div class="quote">
    <%= link_to quote.name, quote_path(quote) %>
    <div class="quote__actions">
      <%= button_to "Delete",
                    quote_path(quote),
                    method: :delete,
                    class: "btn btn--light bg-red-600" %>
      <%= link_to "Edit",
                  edit_quote_path(quote),
                  class: "btn btn--light bg-green-600" %>
    </div>
  </div>
<% end %>